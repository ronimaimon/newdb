<tr>
  <td>
    <%= subject.SUBJECT_ID %>
  </td>
    <td>
    <%= f.text_field :SUBJECT_IDENTIFIER, :size=>15%>
  </td>
  <td>
    <%= f.text_field :SUBJECT_NAME ,:size=>15%>
  </td>
  <td>
    <%= f.select :GENDER ,[['F','F'],['M','M']],{ include_blank: true } %>
  </td>
  <td>
    <%= f.select :AGE_GROUP_ID ,AgeGroup.all.collect {|p| [ p.NAME, p.AGE_GROUP_ID ] }.sort,{ include_blank: true },{:class => 'age_g'}%>
  </td>
    <td>
    <%= f.text_field :AGE, :size=>3,:class => 'age' %>
  </td>
  <td>
    <%= f.select :DOMINANT_HAND,[['L','L'],['R','R']],{ include_blank: true },{:class => "hand"} %>
  </td>
    <td>
    <%= f.check_box :CONTROL , :class => "control"%>
  </td>
    <td>
    <%= f.text_field :tag_list, :class => "token_input tag", "data-url" => "utils/taglist.json", "data-pre" => subject.tag_list.split(",").collect{|t| {:id => t, :name => t }}.to_json %>
  </td>

</tr>
